{
  "name": "ensure-value",
  "version": "1.0.3",
  "description": "A simple lightweight library for ensuring typed values.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "build": "tsc && npx babel ./src --extensions '.ts' -d dist",
    "build:watch": "tsc -w",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abitofdev/ensure-value.git"
  },
  "keywords": [
    "ensure",
    "value",
    "type",
    "protector",
    "guard",
    "validation",
    "typescript"
  ],
  "author": "Ashley Canham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abitofdev/ensure-value/issues"
  },
  "homepage": "https://github.com/abitofdev/ensure-value#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jasmine": "^3.4.4",
    "jasmine": "^3.5.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
