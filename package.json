{
  "name": "ensure-value",
  "version": "1.0.0",
  "description": "A simple lightweight library for ensuring typed values.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "build": "tsc -t ES5",
    "build:watch": "tsc -w -t ES5",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abitofdev/type-protector.git"
  },
  "keywords": [
    "ensure",
    "value",
    "type",
    "protector",
    "guard",
    "validation",
    "typescript"  
  ],
  "author": "Ashley Canham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abitofdev/type-protector/issues"
  },
  "homepage": "https://github.com/abitofdev/type-protector#readme",
  "devDependencies": {
    "@types/jasmine": "^3.4.4",
    "jasmine": "^3.5.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "files": [
    "dist/**/*"
  ]
}
